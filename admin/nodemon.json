{
  "watch": ["src"],
  "ext": "ts,json",
  "ignore": ["src/**/*.spec.ts"],
  "exec1": "ts-node ./src/server.ts",
  "exec2": "ts-node -r tsconfig-paths/register -r ts-node/register src/server.ts",
  "exec3": "node -r tsconfig-paths/register -r ts-node/register src/server.ts",
  "env": {
    "NODE_ENV": "development"
  },
  "exec": "node -r module-alias/register -r ts-node/register src/server.ts"
}
